TOOLCHAIN_ITEMS=(
	'mingw_w64_headers,$shortname'
	'mingw_w64_crt,$shortname'
	'binutils,$shortname'
	'gcc,$shortname'
	'mingw_w64_winpthreads,$shortname'
)


function custom_toolchain_setup {
		pushd $BUILDROOT/$shortname/$target/lib
	  for i in *.dll.a; do 
			F="${i%%.*}"
			if [ ! -e $F.a ]; then
				ln -s $i $F.a
			fi
		done
		
		popd
}
